

BEGIN MRBAYES;

[Load matrix]
exe test1.nex;

[Preliminar Sets]
log start filename = test1.txt;

[Models]
lset rates = gamma coding = variable; 

[Priors]
prset brlenspr = Unconstrained:Exp(10) ratepr = variable;

[MCMCMCMC]
mcmc ngen = 5000000 nruns = 2 nchains = 4 printfreq = 1000 samplefreq = 1000 diagnfreq = 1000 burninfrac = 0.25 filename = test1;

[Summary of tree topologies and branch lengths]
sumt filename = test1;

[Summary of parameters]
sump filename = test1;

END;
